<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <form action="/" method="post" enctype="multipart/form-data">
      <input type="text" name="title">
      <input type="file" name="photo">
      <button>Create Post</button>
    </form>
    <% if (posts) { %>
      <% posts.forEach(post => { %>
        <h3><%= post.title %></h3>
        <img src="<%= post.imageUrl %>" alt="">
      <% }); %>
    <% } %>
  </body>
</html>
